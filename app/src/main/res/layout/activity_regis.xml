<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Regis">

    <EditText
        android:id="@+id/editText3"
        android:layout_width="335dp"
        android:layout_height="48dp"
        android:ems="10"
        android:fontFamily="@font/rubik"
        android:gravity="left"
        android:inputType="textPersonName"
        android:text="Contoh : 12"
        android:textColor="#BABABB"
        android:textColorHighlight="#BABABB"
        android:textColorHint="#BABABB"
        android:textColorLink="#BABABB"
        android:textCursorDrawable="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.325"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.456" />

    <TextView
        android:id="@+id/txt7"
        android:layout_width="290dp"
        android:layout_height="49dp"
        android:fontFamily="@font/rubik"
        android:gravity="left"
        android:text="Nama Panggilan Kamu"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="335dp"
        android:layout_height="48dp"
        android:ems="10"
        android:fontFamily="@font/rubik"
        android:gravity="left"
        android:inputType="textPersonName"
        android:text="Contoh : Budi"
        android:textColor="#BABABB"
        android:textColorHighlight="#BABABB"
        android:textColorHint="#BABABB"
        android:textColorLink="#BABABB"
        android:textCursorDrawable="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.325"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.189" />

    <TextView
        android:id="@+id/txt8"
        android:layout_width="290dp"
        android:layout_height="49dp"
        android:fontFamily="@font/rubik"
        android:gravity="left"
        android:text="Umur Kamu"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.343" />

    <Button
        android:id="@+id/next"
        android:layout_width="355dp"
        android:layout_height="52dp"
        android:background="@drawable/home"
        android:fontFamily="@font/rubik"
        android:text="Selanjutnya"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.923" />

</androidx.constraintlayout.widget.ConstraintLayout>